                    <apex:column headerValue="Discount Type" value="{!errors.data.discountType}" />
                    
                    <apex:column headerValue="Discount %" value="{!errors.data.discountPercent}" />
                    
                    <apex:column headerValue="Net Price" value="{!errors.data.netPrice}" />
                    
