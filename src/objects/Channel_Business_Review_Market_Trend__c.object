    <validationRules>
        <fullName>Channel_Business_Review_Closed_Check</fullName>
        <active>true</active>
        <description>R5 - If Channel Business review Status is Obsolete/Completed, then the user should not edit the Channel Business review Market Trend</description>
        <errorConditionFormula>OR(ISPICKVAL( Channel_Business_Review__r.Status__c , &apos;Obsolete&apos;),
ISPICKVAL( Channel_Business_Review__r.Status__c , &apos;Completed&apos;))</errorConditionFormula>
        <errorMessage>You cannot save the changes - Channel Partner Business Review is Closed</errorMessage>
    </validationRules>
