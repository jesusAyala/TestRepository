        <description>Validation on ‘valid to’ should not be past</description>
        <errorConditionFormula>RecordType.Name =  $Setup.APTS_Agreement_Record_Type__c.APTS_Default_Record_Type__c &amp;&amp; (ISNEW() || ISCHANGED(Apttus__Contract_End_Date__c)) &amp;&amp; Apttus__Contract_End_Date__c &lt;= TODAY()</errorConditionFormula>
        <errorMessage>Valid To should be Future Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APTS_ValidFromCannotbeinPast</fullName>
        <active>false</active>
        <description>valid from date cannot be in the past.</description>
        <errorConditionFormula>Apttus__Contract_Start_Date__c  &lt; TODAY() &amp;&amp;  CreatedDate &lt; NOW() &amp;&amp;  Apttus__Workflow_Trigger_Created_From_Clone__c =false &amp;&amp;  ISCHANGED(Apttus__Contract_Start_Date__c)</errorConditionFormula>
        <errorDisplayField>Apttus__Contract_Start_Date__c</errorDisplayField>
        <errorMessage>MSPA Valid from date cannot be less than today&apos;s date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APTS_Make_ReadOnly_MSPA</fullName>
        <active>true</active>
        <description>The MSPA status is updated to ‘Rejected’. No further changes can be done by Sales User cannot edit the MSPA at this status. The only way to move forward will be to ‘Clone’ the quotation.</description>
        <errorConditionFormula>IF(
$Profile.Name &lt;&gt; &apos;System Administrator&apos; &amp;&amp;
RecordType.Name =  $Setup.APTS_Agreement_Record_Type__c.APTS_Default_Record_Type__c ,
(ISPICKVAL(Apttus__Status__c, &apos;Rejected&apos;) || ISPICKVAL(Apttus__Status__c, &apos;Approved but failed to load on SAP&apos;) || ISPICKVAL(Apttus__Status__c, &apos;Approved but Action Required&apos;) || (ISPICKVAL(Apttus__Status__c, &apos;Recalled&apos;) &amp;&amp;  $User.Id !=  OwnerId ) || ISPICKVAL(Apttus__Status__c, &apos;Generated&apos;) || ISPICKVAL(Apttus__Status__c, &apos;Presented&apos;) ||
ISPICKVAL(Apttus__Status__c, &apos;Expired&apos;) || ISPICKVAL(Apttus__Status__c, &apos;Blocked&apos;) || ISPICKVAL(Apttus__Status__c, &apos;Approved&apos;) ||
ISPICKVAL(Apttus__Status__c, &apos;Approval In Progress&apos;)) &amp;&amp; 
(ISCHANGED(APTS_SAP_Reference_No__c) ||
ISCHANGED(Name) ||
ISCHANGED(Apttus__Subtype__c) ||
ISCHANGED(APTS_LP_Sales_Person__c) ||
ISCHANGED(OwnerId) ||
ISCHANGED(APTS_LP_MSPA_title__c) ||
ISCHANGED(APTS_Estimated_Delivery_Time__c) ||
ISCHANGED(APTS_LP_Price_Date__c) ||
ISCHANGED(APTS_LP_AccountCustomer__c) ||
ISCHANGED(APTS_LP_Account_End_User__c) ||
ISCHANGED(Apttus__Primary_Contact__c) ||
ISCHANGED(APTS_SAP_Customer_ID__c) ||
ISCHANGED(Apttus__Related_Opportunity__c) ||
ISCHANGED(APTS_Account_End_User__c) ||
ISCHANGED(APTS_LP_Sales_Organization__c) ||
ISCHANGED(APTS_Payment_Term_Description__c) ||
ISCHANGED(APTS_Local_Channel_Description__c) ||
ISCHANGED(Apttus__Contract_Start_Date__c) ||
ISCHANGED(Apttus__Contract_End_Date__c) ||
ISCHANGED(Apttus__Description__c) ||
ISCHANGED(APTS_Distribution_Channel__c) ||
ISCHANGED(APTS_Incoterm_Description__c) ||
ISCHANGED(APTS_LP_Max_Order_Amount__c) ||
ISCHANGED(APTS_Sales_Office__c)), false)</errorConditionFormula>
        <errorMessage>Field values cannot be changed when Agreement Status is Approved but action required or Approved or Approved but failed to load in SAP or Rejected or Deactivated or Expired or Blocked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APTS_MaxOrderAmountValidation</fullName>
        <active>true</active>
        <description>Validation to make sure that the max order amount should not be negative amount</description>
        <errorConditionFormula>RecordType.Name =  $Setup.APTS_Agreement_Record_Type__c.APTS_Default_Record_Type__c &amp;&amp; APTS_LP_Max_Order_Amount__c  &lt; 0</errorConditionFormula>
        <errorMessage>Max order amount should not be negative value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APTS_ValidFromDateCheck</fullName>
        <active>true</active>
        <description>Valid From Date cannot be greater than Valid To Date.</description>
        <errorConditionFormula>RecordType.Name =  $Setup.APTS_Agreement_Record_Type__c.APTS_Default_Record_Type__c &amp;&amp; Apttus__Contract_End_Date__c &lt; Apttus__Contract_Start_Date__c</errorConditionFormula>
        <errorMessage>Valid To date cannot be lesser than Valid From Date.</errorMessage>
    </validationRules>
	<validationRules>
        <fullName>Apttus__End_Date_Check</fullName>
        <active>true</active>
        <description>Check that either perpetual is checked or end date is filled in when agreement goes into effect</description>
        <errorConditionFormula>AND(
ISPICKVAL(Apttus__Status_Category__c,&quot;In Effect&quot;),
AND(ISNULL(Apttus__Contract_End_Date__c),NOT(Apttus__Perpetual__c)),
OR(ISNULL(Apttus__Term_Months__c),Apttus__Term_Months__c&lt;=0)
)</errorConditionFormula>
        <errorDisplayField>Apttus__Contract_End_Date__c</errorDisplayField>
        <errorMessage>Please enter the Agreement End Date or mark the Agreement as Perpetual prior to executing the agreement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apttus__Term_Validation</fullName>
        <active>true</active>
        <description>Validate that Term is entered correctly</description>
        <errorConditionFormula>AND( ( Apttus__Perpetual__c ),  (  Apttus__Term_Months__c  ) &gt; 0 )</errorConditionFormula>
        <errorDisplayField>Apttus__Term_Months__c</errorDisplayField>
        <errorMessage>Cannot enter both Perpetual and Term.  Please enter only one of the values</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apttus__Term_Validation_2</fullName>
        <active>true</active>
        <description>Validate that Term is entered correctly</description>
        <errorConditionFormula>AND( (Apttus__Perpetual__c ),  NOT(ISNULL(  Apttus__Contract_End_Date__c )) )</errorConditionFormula>
        <errorDisplayField>Apttus__Contract_End_Date__c</errorDisplayField>
        <errorMessage>Cannot enter both Perpetual and End Date.  Please enter only one of the values</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apttus__Term_Validation_3</fullName>
        <active>false</active>
        <description>Validate that Term is entered correctly (DEPRECATED)</description>
        <errorConditionFormula>AND( (  Apttus__Term_Months__c ) &gt; 0,  NOT(ISNULL(  Apttus__Contract_End_Date__c )) )</errorConditionFormula>
        <errorDisplayField>Apttus__Contract_End_Date__c</errorDisplayField>
        <errorMessage>Cannot enter both Term and End Date.  Please enter only one of the values</errorMessage>
    </validationRules>
