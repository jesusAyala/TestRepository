    <validationRules>
        <fullName>APTPS_Validate_ReadOnly_Fields</fullName>
        <active>false</active>
        <errorConditionFormula>$Profile.Name &lt;&gt; &apos;System Administrator&apos; &amp;&amp; RecordType.Name = &quot;MSPA&quot; &amp;&amp; ( TEXT(Apttus__Status__c)  =&quot;Approved but Action Required&quot; || TEXT(Apttus__Status__c) =&quot;Approved&quot; || TEXT(Apttus__Status__c) =&quot;Approved but failed to load on SAP&quot; || TEXT(Apttus__Status__c) =&quot;Rejected&quot; || TEXT(Apttus__Status__c) =&quot;Deactivated&quot; || TEXT(Apttus__Status__c) =&quot;Expired&quot; || TEXT(Apttus__Status__c )=&quot;Blocked&quot; ) &amp;&amp; (ISCHANGED(APTS_SAP_Reference_No__c) || ISCHANGED(Name) || ISCHANGED(Apttus__Subtype__c)  || ISCHANGED(APTS_LP_Max_Order_Amount__c) || ISCHANGED(APTS_LP_Sales_Person__c) || ISCHANGED(OwnerId) || ISCHANGED(APTS_LP_MSPA_title__c) || ISCHANGED(APTS_Estimated_Delivery_Time__c) || ISCHANGED(APTS_LP_Price_Date__c) || ISCHANGED(Apttus__Primary_Contact__c) )</errorConditionFormula>
        <errorMessage>Field values cannot be changed when Agreement Status is Approved but action required or Approved or Approved but failed to load in SAP or Rejected or Deactivated or Expired or Blocked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APTS_DisableEditDuringAfterApproval</fullName>
        <active>false</active>
        <description>This will not work as need to have changes in the condition</description>
        <errorConditionFormula>AND(  RecordType.Name = &quot;MSPA&quot;,
OR(ISPICKVAL(Apttus__Status__c,&apos;Approved&apos;),
ISPICKVAL(Apttus__Status__c,&apos;Approved but failed to load on SAP&apos;),
ISPICKVAL(Apttus__Status__c,&apos;Approved but Action Required&apos;),
ISPICKVAL(Apttus__Status__c,&apos;Expired&apos;),
ISPICKVAL(Apttus__Status__c,&apos;Blocked&apos;),
ISPICKVAL(Apttus__Status__c,&apos;Saturated&apos;),
ISPICKVAL(Apttus__Status__c,&apos;Deactivated&apos;)
), 
NOT(ISNEW()),
NOT(Apttus__Is_System_Update__c)

)</errorConditionFormula>
        <errorMessage>Can not edit an Agreement after Approval</errorMessage>
    </validationRules>   
