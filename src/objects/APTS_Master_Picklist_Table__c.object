    <validationRules>
        <fullName>Valid_Name_Format</fullName>
        <active>true</active>
        <description>Validation rule to enforce user to enter valid name for the record in the format of &quot;Code - Description&quot;, if code description is not blank. If it is blank, name should be code itself.</description>
        <errorConditionFormula>IF( NOT( ISBLANK( APTS_Code_Description__c ) ) ,  Name  &lt;&gt;  APTS_Code__c  +  &apos; - &apos;  +  APTS_Code_Description__c ,  Name  &lt;&gt;  APTS_Code__c )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Name should be in format of &quot;&lt;code&gt; - &lt;description&gt;&quot;. In case of blank description, name should be &lt;code&gt; itself.</errorMessage>
    </validationRules>
