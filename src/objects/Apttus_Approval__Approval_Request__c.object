    </fields>
    <fields>
        <fullName>APTS_LP_Rejected_Comments_filled__c</fullName>
        <description>Copy the Rejected Comments filled</description>
        <externalId>false</externalId>
        <label>Rejected Comments filled</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>APTS_LP_Rejection_Comments__c</fullName>
        <externalId>false</externalId>
        <formula>NOT CONTAINS(APTS_LP_Rejected_Comments_filled__c,&quot;comments&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Rejection Comments</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
