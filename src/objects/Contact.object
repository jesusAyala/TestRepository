    <validationRules>
        <fullName>Inactive_Contacts</fullName>
        <active>true</active>
        <description>This validation does not allow Edition of Contact Name for Inactive records.</description>
        <errorConditionFormula>AND (NOT($User.AccountMerge_Override__c), Active__c  = FALSE, OR (ISCHANGED ( FirstName ), ISCHANGED ( LastName ), ISCHANGED (  Salutation  )))</errorConditionFormula>
        <errorDisplayField>Active__c</errorDisplayField>
        <errorMessage>Inactive Contacts cannot be Edited!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inconsistent_Contact_Method</fullName>
        <active>true</active>
        <description>Checks whether the preferred Contact Method is consistent with the Opt Out Check boxes</description>
        <errorConditionFormula>AND(NOT($User.AccountMerge_Override__c),
NOT($Setup.Validation_Override__c.Account_Merge_Override__c) ,OR( 
AND(TEXT(Preferred_Contact_Method__c) =&quot;Email&quot;, Email_Opt_Out__c = true), 
AND(TEXT(Preferred_Contact_Method__c) =&quot;Phone&quot;, Phone_Opt_Out__c = true), 
AND(TEXT(Preferred_Contact_Method__c) =&quot;Fax&quot;, Fax_Opt_Out__c = true), 
AND(TEXT(Preferred_Contact_Method__c) =&quot;Mail&quot;, Mail_Opt_Out__c = true),
AND(TEXT(Preferred_Contact_Method__c) =&quot;SMS&quot;, SMS_Opt_Out__c = true)
))</errorConditionFormula>
        <errorDisplayField>Preferred_Contact_Method__c</errorDisplayField>
        <errorMessage>Either remove the Opt Out for this contact method or choose a different contact method.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_New_Contacts_on_Inactive_Accounts</fullName>
        <active>true</active>
        <description>New contacts must not be created if Account is on &quot;Read Only&quot;</description>
        <errorConditionFormula>IF( AND(NOT($User.AccountMerge_Override__c),( Account.RecordType.Name = &quot;Read Only&quot;), ISNEW() ), True, False)</errorConditionFormula>
        <errorMessage>You can not create a new Contact on an Account that is set to Inactive!</errorMessage>
    </validationRules>
