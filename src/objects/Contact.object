        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Open CCRP Dashboards</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/apex.js&quot; )} 


var conid = &apos;{!Contact.Id}&apos;;
var accrectype = &apos;{!Contact.Account_RecordType__c}&apos;; 
var query = &quot;select id,RelatedtoGIS__c, Email from Contact where id=\&apos;&quot;+ conid +&quot;\&apos;&quot;; 
var result = sforce.connection.query(query); 
var records = result.getArray(&quot;records&quot;);
if(accrectype == &apos;GIS Approved&apos;){
    var url = &apos;{!$Label.Contact_CCRP_Dashboard_URL}&apos;; 
    window.open(url +records[0].Email);
}else{
   alert(&apos;This contact is not related to GIS Approved account&apos;);
}</url>
