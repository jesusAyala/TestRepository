    <validationRules>
        <fullName>CBR_Status_Change_to_InApproval</fullName>
        <active>true</active>
        <description>R5 - Validation rule to check the mandatory fields are filled to change the status from Draft to In Approval</description>
        <errorConditionFormula>OR(
AND( 
RecordType.DeveloperName=&quot;Channel_Mid_Period_Business_Review&quot;, OR(ISPICKVAL(Status__c, &quot;In Approval&quot;), ISPICKVAL(Status__c, &quot;Completed&quot;)), OR(ISPICKVAL(Answer_1__c, &quot;None&quot;) ,ISPICKVAL(Answer_1__c, &quot;&quot;), ISPICKVAL(Answer_2__c, &quot;None&quot;), ISPICKVAL(Answer_2__c, &quot;&quot;),  Sales_Answer_1__c = FALSE, Actions_Answer_1__c = FALSE, General_Answer_5__c = FALSE, Sales_Answer_2__c = FALSE, ISBLANK(Actions_Answer_2__c),Partner_Answer_1__c=FALSE ) 
),
AND(
RecordType.DeveloperName=&quot;Channel_End_Period_Business_Review&quot;, OR(ISPICKVAL(Status__c, &quot;In Approval&quot;), ISPICKVAL(Status__c, &quot;Completed&quot;)), OR(ISPICKVAL(Answer_1__c, &quot;None&quot;), ISPICKVAL(Answer_1__c, &quot;&quot;), ISPICKVAL(Answer_2__c, &quot;None&quot;), ISPICKVAL(Answer_2__c, &quot;&quot;), Sales_Answer_1__c = FALSE, Actions_Answer_1__c = FALSE, Sales_Answer_2__c = FALSE, ISBLANK(Actions_Answer_2__c), Partner_Answer_1__c=FALSE, General_Answer_5__c =FALSE)))</errorConditionFormula>
        <errorMessage>Status cannot be changed. Scroll down to fill in the mandatory questions marked with a Red flag to proceed to the next stage.</errorMessage>
    </validationRules>
